{
  "name": "travel-app-backend",
  "version": "1.0.0",
  "description": "Travel App Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec \"ts-node --files src/index.ts\"",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node -e \"const fs = require('fs'); const path = require('path'); const src = 'src/infrastructure/database/migrations'; const dest = 'dist/infrastructure/database/migrations'; if (fs.existsSync(src)) { fs.mkdirSync(dest, { recursive: true }); fs.readdirSync(src).forEach(f => { if (f.endsWith('.sql')) fs.copyFileSync(path.join(src, f), path.join(dest, f)); }); }\"",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "minio": "^7.1.3",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "uuid": "^9.0.1",
    "redis": "^4.6.12",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.7",
    "@types/ioredis": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}



